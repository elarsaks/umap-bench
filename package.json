{
  "name": "umap-bench",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://elarsaks.github.io/umap-bench/",
  "repository": {
    "type": "git",
    "url": "https://github.com/elarsaks/umap-bench.git"
  },
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "postinstall": "mkdir -p public/wasm/pkg && cp -r node_modules/@elarsaks/umap-wasm/wasm/pkg/* public/wasm/pkg/",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "bench": "node scripts/run-benchmarks.cjs --runs=1",
    "bench:headed": "playwright test --headed",
    "bench:ui": "playwright test --ui",
    "bench:loop": "node scripts/run-benchmarks.cjs",
    "bench:loop:small": "node scripts/run-benchmarks.cjs --scope=small",
    "bench:loop:mid": "node scripts/run-benchmarks.cjs --scope=mid",
    "bench:loop:large": "node scripts/run-benchmarks.cjs --scope=large",
    "bench:small:3": "yarn bench:loop --scope=small --runs=3 && yarn bench:loop --scope=small --runs=3 --wasm=all",
    "bench:mid:3": "yarn bench:loop --scope=mid --runs=3 && yarn bench:loop --scope=mid --runs=3 --wasm=all",
    "bench:large:3": "yarn bench:loop --scope=large --runs=3 && yarn bench:loop --scope=large --runs=3 --wasm=all",
    "bench:all:3": "yarn bench:small:3 && yarn bench:mid:3 && yarn bench:large:3"
  },
  "dependencies": {
    "@elarsaks/umap-wasm": "^0.3.8",
    "plotly.js": "^3.3.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-plotly.js": "^2.6.0",
    "styled-components": "^6.3.1",
    "umap-js": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/plotly.js": "^3.0.9",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/styled-components": "^5.1.36",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}
